# GCP Configuration
GCP_PROJECT_ID=uppdemos
GCP_REGION=us-central1
BIGQUERY_DATASET=autonation_demo
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Third-Party APIs (use "MOCK" for development without real APIs)
KBB_API_KEY=MOCK
KBB_API_URL=https://api.kbb.com/v1
CARGURUS_API_KEY=MOCK
CARGURUS_API_URL=https://api.cargurus.com/v1
GOOGLE_MAPS_API_KEY=your-maps-api-key-here

# Gemini Models (ADK will use these)
GEMINI_FLASH_MODEL=gemini-2.5-flash
GEMINI_PRO_MODEL=gemini-2.5-pro
GEMINI_3_FLASH_MODEL=gemini-3-flash-preview

# Application Configuration
STREAMLIT_PORT=8501
LOG_LEVEL=INFO
ENABLE_CACHING=true
CACHE_TTL_MINUTES=15
